# Example environment variables for quantTrader
# Copy this to .env and fill in your values, or use config.json

# Backend API base URL (no trailing slash)
TRADER_API_BASE_URL=http://localhost:8000/api

# Bearer token from POST /api/user/login
# IMPORTANT: Never commit real tokens!
TRADER_API_TOKEN=your_access_token_here

# Polling interval in seconds (optional)
TRADER_POLL_INTERVAL=1.0

# Logging level (optional): DEBUG, INFO, WARNING, ERROR, CRITICAL
TRADER_LOG_LEVEL=INFO
